{
  "editor.minimap.enabled": false,
  "editor.wordWrap": "off", 
  "editor.scrollbar.horizontal": "auto", 
  "editor.scrollbar.vertical": "auto",  
  "editor.codeLens": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.quickSuggestionsDelay": 50,
  "editor.hover.enabled": true,
  "editor.hover.delay": 500,
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek", 
  "editor.gotoLocation.multipleImplementations": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleReferences": "gotoAndPeek",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/build/**": true,
    "**/dist/**": true,  
    "**/.next/**": true, 
    "**/.prisma/client/**": true 
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/build": true,
    "**/dist": true,
    "**/.next": true,
    "**/.prisma/client": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.next": true,
    "**/build": true,
    "**/dist": true,
    "**/.git": true,
    "**/.prisma/client": true
  },
  "explorer.compactFolders": false,
  "telemetry.telemetryLevel": "off",
  "update.mode": "manual", 
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": 10,
  "workbench.editor.limit.value": 20, 
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "none",
  "workbench.statusBar.feedback.enabled": false,
  "git.autofetch": false,
  "git.enabled": true, 
"editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.cursorBlinking": "solid", 
  "editor.smoothScrolling": false, 

  "typescript.disableAutomaticTypeAcquisition": true, 
  "typescript.tsserver.maxTsServerMemory": 4096,
  "javascript.suggest.autoImports": false, 
  "typescript.tsserver.log": "off", 
  "typescript.tsserver.trace": "off", 
  "javascript.updateImportsOnFileMove.enabled": "never", 
  "typescript.updateImportsOnFileMove.enabled": "never",
  "javascript.referencesCodeLens.enabled": false,
  "typescript.referencesCodeLens.enabled": false,
  "javascript.implementationsCodeLens.enabled": false,
  "typescript.implementationsCodeLens.enabled": false,
  "disable-hardware-acceleration": true
}